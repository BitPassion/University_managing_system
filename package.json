{
  "name": "university-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "test": "jest --verbose"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "axios-extensions": "^3.1.2",
    "core-js": "^3.6.4",
    "firebase": "^7.11.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vue-toasted": "^1.1.28",
    "vuetify": "^2.2.11",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-unit-jest": "^4.2.3",
    "@vue/cli-service": "~4.2.0",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "express": "^4.17.1",
    "jest": "^25.1.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "serve-static": "^1.14.1",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "testMatch": [
      "**/tests/**/*.js?(x)"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest"
    },
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
